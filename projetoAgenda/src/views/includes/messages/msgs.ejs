<% if(errors.length > 0) { %>
<div class="row">
    <div class="col">
        <div class="alert alert-danger my-3">
            <% errors.forEach(erro =>{ %>
                <%= erro %>
            <% }); %><br>
        </div>
    </div>
</div>
<% } %>

<% if(success.length > 0) { %>
    <div class="row">
        <div class="col">
            <div class="alert alert-success my-3">
                <% success.forEach(succes =>{ %>
                    <%= success %>
                <% }); %><br>
            </div>
        </div>
    </div>
    <% } %>

